<link rel="preconnect" href="https://fonts.googleapis.com">
<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
<link href="https://fonts.googleapis.com/css2?family=JetBrains+Mono:wght@400;600;700&family=Outfit:wght@400;500;600;700&display=swap" rel="stylesheet">
<style>
  :root {
    --bg-primary: #0a0e17;
    --bg-secondary: #111827;
    --bg-card: #1a2332;
    --bg-hover: #243044;
    --bg-input: #0d1117;
    --bg-code: #0d1117;
    --accent-green: #10b981;
    --accent-green-dim: #065f46;
    --accent-red: #ef4444;
    --accent-red-dim: #7f1d1d;
    --accent-yellow: #f59e0b;
    --accent-blue: #3b82f6;
    --accent-purple: #8b5cf6;
    --text-primary: #f9fafb;
    --text-secondary: #9ca3af;
    --text-muted: #6b7280;
    --border: #374151;
    --shadow: rgba(0, 0, 0, 0.4);
  }

  * {
    margin: 0;
    padding: 0;
    box-sizing: border-box;
  }

  body {
    font-family: 'Outfit', system-ui, sans-serif;
    background: var(--bg-primary);
    color: var(--text-primary);
    min-height: 100vh;
    line-height: 1.6;
  }

  .container {
    max-width: 1400px;
    margin: 0 auto;
    padding: 2rem;
  }

  /* Header */
  .header {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 2rem;
    padding-bottom: 1.5rem;
    border-bottom: 1px solid var(--border);
  }

  .header-left {
    display: flex;
    align-items: center;
    gap: 1.5rem;
  }

  .logo {
    display: flex;
    align-items: center;
    gap: 1rem;
  }

  .logo-icon {
    font-size: 2.5rem;
  }

  .logo-text h1 {
    font-size: 1.75rem;
    font-weight: 700;
    background: linear-gradient(135deg, var(--accent-green), var(--accent-blue));
    -webkit-background-clip: text;
    -webkit-text-fill-color: transparent;
    background-clip: text;
  }

  .logo-text p {
    color: var(--text-muted);
    font-size: 0.875rem;
  }

  .header h1 {
    font-size: 1.5rem;
    font-weight: 700;
    display: flex;
    align-items: center;
    gap: 0.75rem;
  }

  .timestamp, .result-count {
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.875rem;
    color: var(--text-muted);
  }

  .result-count {
    background: var(--bg-card);
    padding: 0.5rem 1rem;
    border-radius: 6px;
  }

  .back-link {
    color: var(--text-muted);
    text-decoration: none;
    font-size: 0.875rem;
    display: flex;
    align-items: center;
    gap: 0.5rem;
    transition: color 0.2s;
  }

  .back-link:hover {
    color: var(--accent-green);
  }

  .breadcrumb {
    margin-bottom: 1.5rem;
  }

  .breadcrumb a {
    color: var(--accent-green);
    text-decoration: none;
  }

  .breadcrumb a:hover {
    text-decoration: underline;
  }

  /* Stats Grid */
  .stats-grid {
    display: grid;
    grid-template-columns: repeat(auto-fit, minmax(200px, 1fr));
    gap: 1rem;
    margin-bottom: 2rem;
  }

  .stat-card {
    background: var(--bg-card);
    border-radius: 12px;
    padding: 1.5rem;
    border: 1px solid var(--border);
    transition: all 0.2s ease;
    text-decoration: none;
    display: block;
    cursor: pointer;
  }

  .stat-card:hover {
    border-color: var(--accent-green-dim);
    box-shadow: 0 0 20px rgba(16, 185, 129, 0.1);
    transform: translateY(-2px);
  }

  .stat-label {
    font-size: 0.875rem;
    color: var(--text-muted);
    margin-bottom: 0.5rem;
    text-transform: uppercase;
    letter-spacing: 0.05em;
  }

  .stat-value {
    font-size: 2rem;
    font-weight: 700;
    font-family: 'JetBrains Mono', monospace;
  }

  .stat-value.green { color: var(--accent-green); }
  .stat-value.red { color: var(--accent-red); }
  .stat-value.yellow { color: var(--accent-yellow); }
  .stat-value.blue { color: var(--accent-blue); }
  .stat-value.purple { color: var(--accent-purple); }

  /* Sections */
  .section {
    margin-bottom: 2rem;
  }

  .section-header {
    display: flex;
    align-items: center;
    justify-content: space-between;
    margin-bottom: 1rem;
  }

  .section-title {
    font-size: 1.25rem;
    font-weight: 600;
    display: flex;
    align-items: center;
    gap: 0.5rem;
  }

  /* Cards */
  .card {
    background: var(--bg-card);
    border-radius: 12px;
    border: 1px solid var(--border);
    overflow: hidden;
  }

  .card-header {
    padding: 1rem 1.5rem;
    border-bottom: 1px solid var(--border);
    background: var(--bg-secondary);
    border-radius: 12px 12px 0 0;
    font-weight: 600;
    display: flex;
    align-items: center;
    gap: 0.5rem;
  }

  .card-body {
    padding: 1.5rem;
  }

  /* Tables */
  .table-wrapper {
    overflow-x: auto;
  }

  table {
    width: 100%;
    border-collapse: collapse;
    font-size: 0.875rem;
  }

  th {
    text-align: left;
    padding: 0.875rem 1rem;
    font-weight: 600;
    color: var(--text-secondary);
    text-transform: uppercase;
    font-size: 0.75rem;
    letter-spacing: 0.05em;
    background: var(--bg-secondary);
    border-bottom: 1px solid var(--border);
  }

  td {
    padding: 0.75rem 1rem;
    border-bottom: 1px solid var(--border);
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.8125rem;
  }

  tr:hover {
    background: var(--bg-hover);
  }

  tr:last-child td {
    border-bottom: none;
  }

  /* Badges */
  .badge {
    display: inline-flex;
    align-items: center;
    padding: 0.25rem 0.625rem;
    border-radius: 9999px;
    font-size: 0.75rem;
    font-weight: 600;
    font-family: 'JetBrains Mono', monospace;
  }

  .badge-green { background: var(--accent-green-dim); color: var(--accent-green); }
  .badge-red { background: var(--accent-red-dim); color: var(--accent-red); }
  .badge-yellow { background: rgba(245, 158, 11, 0.2); color: var(--accent-yellow); }
  .badge-blue { background: rgba(59, 130, 246, 0.2); color: var(--accent-blue); }
  .badge-purple { background: rgba(139, 92, 246, 0.2); color: var(--accent-purple); }

  /* IP Cell */
  .ip-cell {
    display: flex;
    align-items: center;
    gap: 0.5rem;
  }

  /* Buttons */
  .btn {
    display: inline-flex;
    align-items: center;
    justify-content: center;
    gap: 0.375rem;
    padding: 0.625rem 1rem;
    border-radius: 6px;
    font-size: 0.875rem;
    font-weight: 500;
    cursor: pointer;
    border: none;
    transition: all 0.2s ease;
    text-decoration: none;
  }

  .btn-sm {
    padding: 0.25rem 0.5rem;
    font-size: 0.75rem;
  }

  .btn-primary {
    background: var(--accent-green);
    color: white;
  }

  .btn-primary:hover {
    background: #0d9668;
  }

  .btn-secondary {
    background: var(--bg-secondary);
    color: var(--text-secondary);
    border: 1px solid var(--border);
  }

  .btn-secondary:hover {
    background: var(--bg-hover);
    color: var(--text-primary);
  }

  .btn-danger {
    background: var(--accent-red-dim);
    color: var(--accent-red);
  }

  .btn-danger:hover {
    background: var(--accent-red);
    color: white;
  }

  .btn-success {
    background: var(--accent-green-dim);
    color: var(--accent-green);
  }

  .btn-success:hover {
    background: var(--accent-green);
    color: white;
  }

  /* Actions */
  .actions {
    display: flex;
    gap: 0.5rem;
  }

  /* Grid layouts */
  .two-col {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 1.5rem;
  }

  .grid {
    display: grid;
    grid-template-columns: repeat(2, 1fr);
    gap: 1rem;
  }

  @media (max-width: 900px) {
    .two-col { grid-template-columns: 1fr; }
  }

  @media (max-width: 600px) {
    .grid { grid-template-columns: 1fr; }
  }

  /* Trap type chart */
  .type-bar {
    display: flex;
    align-items: center;
    gap: 1rem;
    padding: 0.5rem 0;
  }

  .type-name {
    min-width: 160px;
    font-size: 0.8125rem;
  }

  .type-bar-track {
    flex: 1;
    height: 24px;
    background: var(--bg-secondary);
    border-radius: 4px;
    overflow: hidden;
  }

  .type-bar-fill {
    height: 100%;
    border-radius: 4px;
    display: flex;
    align-items: center;
    justify-content: flex-end;
    padding-right: 0.5rem;
    font-size: 0.75rem;
    font-weight: 600;
    font-family: 'JetBrains Mono', monospace;
  }

  .type-bar-fill.green { background: linear-gradient(90deg, var(--accent-green-dim), var(--accent-green)); }
  .type-bar-fill.red { background: linear-gradient(90deg, var(--accent-red-dim), var(--accent-red)); }
  .type-bar-fill.yellow { background: linear-gradient(90deg, rgba(245, 158, 11, 0.3), var(--accent-yellow)); }
  .type-bar-fill.blue { background: linear-gradient(90deg, rgba(59, 130, 246, 0.3), var(--accent-blue)); }
  .type-bar-fill.purple { background: linear-gradient(90deg, rgba(139, 92, 246, 0.3), var(--accent-purple)); }

  /* Filters */
  .filters {
    background: var(--bg-card);
    border-radius: 12px;
    padding: 1.5rem;
    margin-bottom: 1.5rem;
    border: 1px solid var(--border);
  }

  .filters-form {
    display: flex;
    flex-wrap: wrap;
    gap: 1rem;
    align-items: flex-end;
  }

  .filter-group {
    display: flex;
    flex-direction: column;
    gap: 0.375rem;
  }

  .filter-group.search {
    flex: 1;
    min-width: 200px;
  }

  .filter-label {
    font-size: 0.75rem;
    color: var(--text-muted);
    text-transform: uppercase;
    letter-spacing: 0.05em;
  }

  .filter-input, .filter-select {
    background: var(--bg-input);
    border: 1px solid var(--border);
    border-radius: 6px;
    padding: 0.625rem 0.875rem;
    color: var(--text-primary);
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.875rem;
    min-width: 140px;
  }

  .filter-input:focus, .filter-select:focus {
    outline: none;
    border-color: var(--accent-green);
    box-shadow: 0 0 0 2px rgba(16, 185, 129, 0.2);
  }

  .filter-input::placeholder {
    color: var(--text-muted);
  }

  .filter-select {
    cursor: pointer;
  }

  .filter-actions {
    display: flex;
    gap: 0.5rem;
  }

  /* Active filters */
  .active-filters {
    display: flex;
    flex-wrap: wrap;
    gap: 0.5rem;
    margin-top: 1rem;
    padding-top: 1rem;
    border-top: 1px solid var(--border);
  }

  .filter-tag {
    display: inline-flex;
    align-items: center;
    gap: 0.375rem;
    background: var(--accent-green-dim);
    color: var(--accent-green);
    padding: 0.25rem 0.625rem;
    border-radius: 9999px;
    font-size: 0.75rem;
    font-family: 'JetBrains Mono', monospace;
  }

  .filter-tag a {
    color: inherit;
    text-decoration: none;
    opacity: 0.7;
  }

  .filter-tag a:hover {
    opacity: 1;
  }

  /* Pagination */
  .pagination {
    display: flex;
    justify-content: space-between;
    align-items: center;
    padding: 1rem 1.5rem;
    background: var(--bg-secondary);
    border-top: 1px solid var(--border);
  }

  .pagination-info {
    font-size: 0.875rem;
    color: var(--text-muted);
  }

  .pagination-links {
    display: flex;
    gap: 0.25rem;
  }

  .page-link {
    display: inline-flex;
    align-items: center;
    justify-content: center;
    min-width: 2rem;
    height: 2rem;
    padding: 0 0.5rem;
    border-radius: 4px;
    font-size: 0.875rem;
    text-decoration: none;
    color: var(--text-secondary);
    background: var(--bg-card);
    border: 1px solid var(--border);
    transition: all 0.2s;
  }

  .page-link:hover {
    background: var(--bg-hover);
    color: var(--text-primary);
  }

  .page-link.active {
    background: var(--accent-green);
    border-color: var(--accent-green);
    color: white;
  }

  .page-link.disabled {
    opacity: 0.5;
    pointer-events: none;
  }

  /* Empty state */
  .empty-state {
    text-align: center;
    padding: 3rem;
    color: var(--text-muted);
  }

  .empty-state .icon {
    font-size: 3rem;
    margin-bottom: 1rem;
  }

  /* Field display (show page) */
  .field {
    padding: 0.75rem 0;
    border-bottom: 1px solid var(--border);
  }

  .field:last-child {
    border-bottom: none;
  }

  .field-label {
    font-size: 0.75rem;
    color: var(--text-muted);
    text-transform: uppercase;
    letter-spacing: 0.05em;
    margin-bottom: 0.25rem;
  }

  .field-value {
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.875rem;
    word-break: break-all;
  }

  .json-block {
    background: var(--bg-code);
    border-radius: 8px;
    padding: 1rem;
    overflow-x: auto;
    font-family: 'JetBrains Mono', monospace;
    font-size: 0.8125rem;
    line-height: 1.5;
    white-space: pre-wrap;
    color: var(--text-secondary);
  }

  /* Footer */
  .footer {
    margin-top: 3rem;
    padding-top: 1.5rem;
    border-top: 1px solid var(--border);
    text-align: center;
    color: var(--text-muted);
    font-size: 0.875rem;
  }

  .footer a {
    color: var(--accent-green);
    text-decoration: none;
  }

  .footer a:hover {
    text-decoration: underline;
  }

  /* Stat card with trend */
  .stat-value-wrapper {
    display: flex;
    align-items: baseline;
    gap: 0.5rem;
  }

  .trend {
    font-size: 0.75rem;
    font-family: 'JetBrains Mono', monospace;
    padding: 0.125rem 0.375rem;
    border-radius: 4px;
  }

  .trend-up {
    color: var(--accent-green);
    background: var(--accent-green-dim);
  }

  .trend-down {
    color: var(--accent-red);
    background: var(--accent-red-dim);
  }

  .trend-neutral {
    color: var(--text-muted);
    background: var(--bg-secondary);
  }

  /* Relative time */
  .relative-time {
    cursor: help;
  }

  /* Trend Chart (7-day bar chart) */
  .trend-chart {
    padding: 1rem 0;
  }

  .chart-bars {
    display: flex;
    align-items: flex-end;
    justify-content: space-between;
    height: 120px;
    gap: 0.5rem;
    padding: 0 0.5rem;
  }

  .chart-bar-wrapper {
    flex: 1;
    display: flex;
    flex-direction: column;
    align-items: center;
    height: 100%;
  }

  .chart-bar {
    width: 100%;
    min-height: 4px;
    background: linear-gradient(180deg, var(--accent-green), var(--accent-green-dim));
    border-radius: 4px 4px 0 0;
    display: flex;
    align-items: flex-start;
    justify-content: center;
    transition: all 0.2s;
    cursor: pointer;
  }

  .chart-bar:hover {
    filter: brightness(1.2);
  }

  .chart-bar-value {
    font-size: 0.625rem;
    font-family: 'JetBrains Mono', monospace;
    color: var(--text-primary);
    margin-top: 0.25rem;
    text-shadow: 0 1px 2px rgba(0,0,0,0.5);
  }

  .chart-bar-label {
    font-size: 0.6875rem;
    color: var(--text-muted);
    margin-top: 0.375rem;
    text-transform: uppercase;
  }

  /* User Agent Chart */
  .ua-chart {
    display: flex;
    flex-direction: column;
    gap: 0.5rem;
  }

  .ua-bar-row {
    display: flex;
    align-items: center;
    gap: 0.75rem;
  }

  .ua-name {
    min-width: 140px;
    max-width: 140px;
    font-size: 0.75rem;
    font-family: 'JetBrains Mono', monospace;
    color: var(--text-secondary);
    white-space: nowrap;
    overflow: hidden;
    text-overflow: ellipsis;
  }

  .ua-bar-track {
    flex: 1;
    height: 20px;
    background: var(--bg-secondary);
    border-radius: 4px;
    overflow: hidden;
  }

  .ua-bar-fill {
    height: 100%;
    background: linear-gradient(90deg, var(--accent-purple), var(--accent-blue));
    border-radius: 4px;
    display: flex;
    align-items: center;
    justify-content: flex-end;
    padding-right: 0.5rem;
    min-width: fit-content;
  }

  .ua-bar-value {
    font-size: 0.6875rem;
    font-family: 'JetBrains Mono', monospace;
    color: var(--text-primary);
    white-space: nowrap;
  }

  /* Heatmap */
  .heatmap {
    overflow-x: auto;
  }

  .heatmap-header {
    display: flex;
    margin-bottom: 0.25rem;
  }

  .heatmap-corner {
    width: 40px;
    flex-shrink: 0;
  }

  .heatmap-hour {
    width: 18px;
    font-size: 0.5rem;
    color: var(--text-muted);
    text-align: center;
    font-family: 'JetBrains Mono', monospace;
  }

  .heatmap-row {
    display: flex;
    margin-bottom: 2px;
  }

  .heatmap-day {
    width: 40px;
    font-size: 0.6875rem;
    color: var(--text-muted);
    display: flex;
    align-items: center;
    flex-shrink: 0;
  }

  .heatmap-cell {
    width: 16px;
    height: 16px;
    margin: 1px;
    border-radius: 2px;
    cursor: pointer;
    transition: transform 0.1s;
  }

  .heatmap-cell:hover {
    transform: scale(1.3);
  }

  .heat-0 { background: var(--bg-secondary); }
  .heat-1 { background: rgba(16, 185, 129, 0.2); }
  .heat-2 { background: rgba(16, 185, 129, 0.4); }
  .heat-3 { background: rgba(16, 185, 129, 0.6); }
  .heat-4 { background: rgba(16, 185, 129, 0.8); }
  .heat-5 { background: var(--accent-green); }

  .heatmap-legend {
    display: flex;
    align-items: center;
    gap: 0.25rem;
    margin-top: 0.75rem;
    justify-content: flex-end;
  }

  .legend-label {
    font-size: 0.625rem;
    color: var(--text-muted);
    margin: 0 0.25rem;
  }

  .legend-cell {
    width: 12px;
    height: 12px;
    border-radius: 2px;
  }

  /* Three column layout */
  .three-col {
    display: grid;
    grid-template-columns: repeat(3, 1fr);
    gap: 1.5rem;
  }

  @media (max-width: 1200px) {
    .three-col {
      grid-template-columns: repeat(2, 1fr);
    }
  }

  @media (max-width: 768px) {
    .three-col {
      grid-template-columns: 1fr;
    }
  }

  /* View all link */
  .view-all-link {
    display: flex;
    justify-content: center;
    padding: 1rem;
    border-top: 1px solid var(--border);
  }

  .view-all-link a {
    color: var(--accent-green);
    text-decoration: none;
    font-size: 0.875rem;
    display: flex;
    align-items: center;
    gap: 0.375rem;
  }

  .view-all-link a:hover {
    text-decoration: underline;
  }

  /* Export button */
  .header-actions {
    display: flex;
    gap: 0.75rem;
    align-items: center;
  }

  /* Responsive */
  @media (max-width: 768px) {
    .container {
      padding: 1rem;
    }

    .header {
      flex-direction: column;
      gap: 1rem;
      align-items: flex-start;
    }

    .stats-grid {
      grid-template-columns: repeat(2, 1fr);
    }

    .stat-card {
      padding: 1rem;
    }

    .stat-value {
      font-size: 1.5rem;
    }

    .two-col {
      grid-template-columns: 1fr;
    }

    .filters-form {
      flex-direction: column;
    }

    .filter-group {
      width: 100%;
    }

    .filter-actions {
      width: 100%;
    }

    .filter-actions .btn {
      flex: 1;
    }

    .ua-name {
      min-width: 80px;
      max-width: 80px;
    }

    .table-wrapper {
      margin: 0 -1rem;
      padding: 0 1rem;
    }
  }

  @media (max-width: 480px) {
    .stats-grid {
      grid-template-columns: 1fr;
    }

    .chart-bars {
      height: 80px;
    }
  }

  /* Light mode support */
  @media (prefers-color-scheme: light) {
    :root {
      --bg-primary: #f8fafc;
      --bg-secondary: #f1f5f9;
      --bg-card: #ffffff;
      --bg-hover: #e2e8f0;
      --bg-input: #ffffff;
      --bg-code: #f1f5f9;
      --accent-green: #059669;
      --accent-green-dim: #d1fae5;
      --accent-red: #dc2626;
      --accent-red-dim: #fee2e2;
      --accent-yellow: #d97706;
      --accent-blue: #2563eb;
      --accent-purple: #7c3aed;
      --text-primary: #0f172a;
      --text-secondary: #475569;
